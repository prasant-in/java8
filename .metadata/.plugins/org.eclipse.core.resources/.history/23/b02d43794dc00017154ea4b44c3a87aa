package com.yatish.tutorial.Lambda;

public class J14_this_Reference_In_Lambda {

	public void m1() {
		
	}
	
	public static void main(String[] args) {
		
		String label = "outside scope variable";
		
		M mmm123 = () -> {System.out.println(this.label);};
		mmm123.m();

	}

}

interface M {
	void m();
}